{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "Default",
            "hidden": true,
            "displayName": "Default Config",
            "description": "Default build ",
            "binaryDir": "$env{TWWorkspaceHome}/TWBuilds/TWInt/${sourceDirName}/${presetName}",
            "cacheVariables": {
                "TWWorkspaceHome": {
                    "type": "FILEPATH",
                    "value": "$env{TWWorkspaceHome}"
                },
                "CMAKE_MODULE_PATH": {
                    "type": "FILEPATH",
                    "value": "$env{TWWorkspaceHome}/TallyWorld/DevSupportPlatform/TWIDESupport.Build/CMake"
                },
                "CMAKE_CXX_STANDARD": "17",
                "CMAKE_CXX_STANDARD_REQUIRED": "ON",
                "CMAKE_OBJECT_PATH_MAX": "1024",
                "CMAKE_INSTALL_PREFIX": {
                    "type": "FILEPATH",
                    "value": "$env{TWWorkspaceHome}/TWBuilds"
                },
                "CMAKE_ARCHIVE_OUTPUT_DIRECTORY": {
                    "type": "FILEPATH",
                    "value": "$env{TWWorkspaceHome}/TWBuilds/TWLib/${presetName}"
                },
                "CMAKE_LIBRARY_OUTPUT_DIRECTORY": {
                    "type": "FILEPATH",
                    "value": "$env{TWWorkspaceHome}/TWBuilds/TWLib/${presetName}"
                },
                "CMAKE_RUNTIME_OUTPUT_DIRECTORY": {
                    "type": "FILEPATH",
                    "value": "$env{TWWorkspaceHome}/TWBuilds/TWBin/${presetName}"
                },
                "CMAKE_MESSAGE_LOG_LEVEL": "NOTICE"
            },
            "environment": {
                "CXXFLAGS_Default": "$penv{CXXFLAGS} -flto",
                "TWDEBUG": "-DTW_DEBUG=1 -DTW_DIAGS=1 -DTW_SIMULATOR=0 -DTW_METRICS=1 -DTW_TRACE=1",
                "TWSIMULATION": "-DTW_DEBUG=0 -DTW_DIAGS=0 -DTW_SIMULATOR=1 -DTW_METRICS=1 -DTW_TRACE=1",
                "TWMETRICSONLY": "-DTW_DEBUG=0 -DTW_DIAGS=0 -DTW_SIMULATOR=0 -DTW_METRICS=1 -DTW_TRACE=0",
                "TWRELEASE": "-DTW_DEBUG=0 -DTW_DIAGS=0 -DTW_SIMULATOR=0 -DTW_METRICS=0 -DTW_TRACE=0"
            }
        },
        {
            "name": "Ninja",
            "hidden": true,
            "displayName": "Ninjs Config",
            "description": "Default build using Ninja generator",
            "generator": "Ninja Multi-Config",
            "cacheVariables": {
                "CMAKE_CONFIGURATION_TYPES": "TWDebug;TWSimulation;TWMetricsOnly;TWRelease"
            }
        },
        {
            "name": "XCode",
            "hidden": true,
            "displayName": "XCode Config",
            "description": "Default build using XCode generator",
            "generator": "Xcode"
        },
        {
            "name": "Windows",
            "hidden": true,
            "displayName": "Windows Default Config",
            "description": "Windows Default configuration",
            "inherits": [ "Ninja" ],
            "cacheVariables": {
                "TW_BUILD_OS": "Windows",
                "CMAKE_CXX_COMPILER": {
                    "type": "FILEPATH",
                    "value": "$env{LLVMInstallDir}/bin/clang-cl.exe"
                },
                "CMAKE_LINKER": {
                    "type": "FILEPATH",
                    "value": "$env{LLVMInstallDir}/bin/lld-link.exe"
                },
                "CMAKE_AR": {
                    "type": "FILEPATH",
                    "value": "$env{LLVMInstallDir}/bin/llvm-lib.exe"
                },
                "CMAKE_RANLIB": {
                    "type": "FILEPATH",
                    "value": "$env{LLVMInstallDir}/llvm-ranlib.exe"
                }
            },
            "environment": {
                "CXXFLAGS_Windows": "-DTW_OS_WINDOWS=1 -DTW_OS_LINUX_KERNEL=0 -DTW_OS_ANDROID=0 -DTW_OS_MACOS=0 -DTW_OS_IOS=0 /Gv",
                "DEBUGFLAGS": "/Zi /Ob0 /Od /RTC1",
                "RELEASEFLAGS": "/O2 /Ob2 /DNDEBUG"

            }
        },
        {
            "name": "Android",
            "hidden": true,
            "displayName": "Android Default Config",
            "description": "Android Default configuration",
            "inherits": [ "Ninja" ],
            "cacheVariables": {

                "CMAKE_TOOLCHAIN_FILE": "$env{ANDROID_NDK}/build/cmake/android.toolchain.cmake",
                "ANDROID_NATIVE_API_LEVEL": "23",
                "ANDROID_TOOLCHAIN": "clang",
                "TW_BUILD_OS": "Android",
                "ANDROID_SDK_ROOT": "$env{ANDROID_SDK}",
                "ANDROID_NDK": "$env{ANDROID_NDK}"
            },
            "environment": {
                "CXXFLAGS_Android": "-DTW_OS_WINDOWS=0 -DTW_OS_LINUX_KERNEL=1 -DTW_OS_ANDROID=1 -DTW_OS_MACOS=0 -DTW_OS_IOS=0",
                "DEBUGFLAGS": "-O0 -fno-limit-debug-info",
                "RELEASEFLAGS": "-O2 -DNDEBUG"
            }
        },
        {
            "name": "Linux",
            "hidden": true,
