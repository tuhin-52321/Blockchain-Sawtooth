<Window x:Class="LicenseUserApp.AssignLicense"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LicenseUserApp"
        mc:Ignorable="d"
        Title="Assign License" Height="120" Width="200">
    <StackPanel>
        <RadioButton Name="rbSilver" IsChecked="True" Margin="0,8,5,0" GroupName="License" Background="Silver">Silver</RadioButton>
        <RadioButton Name="rbGold"  Margin="0,8,5,5" GroupName="License" Background="Gold" >Gold</RadioButton>
        <Button Content="Assign License" Margin="5,5,5,5" Click="OnSubmit" />
    </StackPanel>
</Window>
